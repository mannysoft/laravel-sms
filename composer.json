{
    "name": "mannysoft/laravel-sms",
    "description": "Sending SMS for multiple services",
    "keywords": ["sms", "laravel", "text messages", "send"],
    "homepage": "https://www.mannyisles.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Manny Isles",
            "email": "manny.isles@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/mail": ">=5.4.0",
        "illuminate/log": ">=5.4.0",
        "illuminate/queue": ">=5.4.0",
        "illuminate/support": ">=5.4.0",
        "illuminate/view": ">=5.4.0",
        "guzzlehttp/guzzle": "~6",
        "twilio/sdk": "^5.12",
        "plivo/plivo-php": "^1.1",
        "smsapi/php-client": "^1.8"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5"
    },
    "autoload": {
        "psr-4": {
            "Mannysoft\\SMS\\": "src/"
        }
    },
    "minimum-stability": "stable"
}